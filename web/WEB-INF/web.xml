<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
		 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		 http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
		 version="2.5">

	<servlet>
		<servlet-name>JetServlet</servlet-name>
		<servlet-class>system.JetServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>JetStaticServlet</servlet-name>
		<servlet-class>system.JetStaticServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>JetServlet</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>JetStaticServlet</servlet-name>
		<url-pattern>/generated/</url-pattern>
	</servlet-mapping>

</web-app>
